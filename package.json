{
  "name": "@trosckey/scraper-utils",
  "version": "0.3.0",
  "description": "helpful tools for process text",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "NODE_OPTIONS='--experimental-specifier-resolution=node --loader ts-node/esm' mocha -r ts-node/register 'test/*.test.ts' --delay",
    "build": "rm -fr dist/* && tsc -p tsconfig-mjs.json && tsc -p tsconfig-cjs.json && ./fixup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trosck/scrapper-utils.git"
  },
  "keywords": [
    "process",
    "text",
    "scraping"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trosck/scrapper-utils/issues"
  },
  "homepage": "https://github.com/trosck/scrapper-utils#readme",
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "mocha": "^10.0.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  }
}
